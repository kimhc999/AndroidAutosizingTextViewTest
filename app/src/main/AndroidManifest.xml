<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hckim.autosizingtextviewtest">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CheckboxTestActivity" />
        <activity android:name=".CheckboxSupportTestActivity" />
        <activity android:name=".EdittextTestActivity" />
        <activity android:name=".EdittextSupportTestActivity" />
        <activity android:name=".ToggleTestActivity" />
        <activity android:name=".ToggleSupportTestActivity" />
    </application>

</manifest>